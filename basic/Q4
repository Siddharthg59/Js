// Lengths of the sides
let a = 5;
let b = 6;
let c = 7;

// Calculate the semi-perimeter
let s = (a + b + c) / 2;

// Calculate the area using Heron's formula
let area = Math.sqrt(s * (s - a) * (s - b) * (s - c));

// Limit area to 3 decimal places
area = area.toFixed(3);

// Print the result
console.log("Area of the triangle is: " + area);
