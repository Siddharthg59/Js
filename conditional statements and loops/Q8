function isHappy(num) {
  let seen = new Set();
  while (num !== 1 && !seen.has(num)) {
    seen.add(num);
    num = num.toString().split('').reduce((sum, digit) => sum + digit ** 2, 0);
  }
  return num === 1;
}

let count = 0, number = 1;
while (count < 5) {
  if (isHappy(number)) {
    console.log(number);
    count++;
  }
  number++;
}
